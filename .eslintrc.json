{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "node"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": 2,
        "no-unused-vars": 1,
        "no-console": 0,
        "no-undef": 0,
        "indent": ["error", "tab"]
    }
}
